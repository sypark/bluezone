  <select id="selectGameWithCustInfo" parameterType="java.lang.Integer" resultMap="CustCompResultMap" >
  SELECT A.*,
        B.CUST_NM,
        B.EMAIL,
        B.NICK_NAME,
        C.SCORE AS CUST_SCORE,
        C.SCORE_AVG AS CUST_SCORE_AVG,
        C.AVG AS CUST_AVG,
        C.HIGH_RUN AS CUST_HIGH_RUN,
        C.WIN_CNT AS CUST_WIN_CNT,
        C.LOSE_CNT AS CUST_LOSE_CNT,
        C.DRAW_CNT AS CUST_DRAW_CNT
  FROM  GAME_RECORD A,
	       CST_CUST_MST B
           LEFT JOIN CST_CUST_RECORD_MST C ON B.CUST_NO=C.CUST_NO 
 WHERE A.GAME_NO = #{gameNo,jdbcType=INTEGER}
  AND A.CUST_NO = B.CUST_NO
  AND B.USE_YN = 'Y'
  </select>
  <select id="selectTotalCustGameRecordList" parameterType="java.lang.Integer" resultMap="CustCompResultMap"  >
  	SELECT B.GAME_DT,
	        B.GAME_NM,
	        B.INNING_CNT,
			(SELECT BB.NICK_NAME
			   FROM GAME_RECORD AA,
					  CST_CUST_MST BB
			WHERE AA.GAME_NO = A.GAME_NO
	          AND AA.CUST_NO = BB.CUST_NO
	          AND BB.CUST_NO != A.CUST_NO
	          LIMIT 0,1
	         ) AS NICK_NAME,
	         (SELECT aa.score
			   FROM GAME_RECORD AA,
					CST_CUST_MST BB
			WHERE AA.GAME_NO = A.GAME_NO
	          AND AA.CUST_NO = BB.CUST_NO
	          AND BB.CUST_NO != A.CUST_NO
			  LIMIT 0,1
	         ) AS CUST_SCORE,
	        A.*
	FROM
		GAME_RECORD A,
		GAME_MST B,
	    CST_CUST_MST C
	WHERE A.CUST_NO = #{custNo,jdbcType=INTEGER}
	  AND A.GAME_NO = B.GAME_NO
	  AND B.STATUS = 'E'
	  AND A.CUST_NO = C.CUST_NO
	ORDER BY B.REG_DTM DESC
	</select>
